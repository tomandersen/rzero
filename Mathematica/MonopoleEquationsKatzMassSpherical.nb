   3.936013762733919*^9}, {3.936013840370821*^9, 3.9360138460452547`*^9}, {
   3.936013905301125*^9, 3.9360139065761127`*^9}, {3.9360140733968883`*^9, 
   3.936014104016035*^9}, {3.942157564954987*^9, 3.94215758074816*^9}},
 CellLabel->
